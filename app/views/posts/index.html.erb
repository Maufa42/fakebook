<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <%# <th></th> %>
      <th>Title</th>
      <th>Like</th>
      <th>Count</th>
      <th>Show</th>
      <th>Edit</th>
      <th>Destroy</th>

    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.name%></td>
        <td><%= link_to "Like",url = "/likes/#{post.id}"%></td>
        <td>0</td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

